.PHONY: clean All

All:
	@echo "----------Building project:[ BARE2DEngine - Debug ]----------"
	@cd "/home/davis-dev/Documents/Programming/C++/CodingGithub/BARE2DEngine" && "$(MAKE)" -f  "BARE2DEngine.mk"
	@echo "----------Building project:[ Fear-Of-The-Dark - Debug ]----------"
	@"$(MAKE)" -f  "Fear-Of-The-Dark.mk"
clean:
	@echo "----------Cleaning project:[ BARE2DEngine - Debug ]----------"
	@cd "/home/davis-dev/Documents/Programming/C++/CodingGithub/BARE2DEngine" && "$(MAKE)" -f  "BARE2DEngine.mk"  clean
	@echo "----------Cleaning project:[ Fear-Of-The-Dark - Debug ]----------"
	@"$(MAKE)" -f  "Fear-Of-The-Dark.mk" clean
