.PHONY: clean All

All:
	@echo "----------Building project:[ GLEngine - Release ]----------"
	@cd "/home/davis-dev/Documents/Programming/C++/CodingGithub/GLEngine" && "$(MAKE)" -f  "GLEngine.mk"
	@echo "----------Building project:[ Fear-Of-The-Dark - Debug ]----------"
	@"$(MAKE)" -f  "Fear-Of-The-Dark.mk"
clean:
	@echo "----------Cleaning project:[ GLEngine - Release ]----------"
	@cd "/home/davis-dev/Documents/Programming/C++/CodingGithub/GLEngine" && "$(MAKE)" -f  "GLEngine.mk"  clean
	@echo "----------Cleaning project:[ Fear-Of-The-Dark - Debug ]----------"
	@"$(MAKE)" -f  "Fear-Of-The-Dark.mk" clean
